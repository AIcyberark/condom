{
    "plugins": ["jest", "@typescript-eslint"],
    "extends": ["plugin:github/recommended", "eslint:recommended", "google", "prettier/@typescript-eslint", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "require-jsdoc": "off",
      "no-console": "off"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest/globals": true
    }
  }